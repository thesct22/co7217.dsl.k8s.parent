---
deployment
---
{
name="dsl-example",
namespace="xyz",
label{
    app="dsl-example"
},

replicas=3,
container{
    name= "busybox",
    image= "gcr.io/google-containers/busybox:latest"
},
container{
    name="backend",
    image="gcr.io/google-containers/defaultbackend:latest"
}
}
---
hpa
---
{
name="hpa",
namespace ="xyz",
label{
    app="dsl-example"
},
target="dsl-example",   
minimum=1,
maximum=5,
metrices{
    cpu=80,
    memory=70
}
}
